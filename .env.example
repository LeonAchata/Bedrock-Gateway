# AWS Bedrock Configuration
# Obten estas credenciales de tu cuenta AWS
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1

# Cache Configuration
# Habilita el cache para reducir costos y latencia
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Metrics Configuration
# Habilita m√©tricas para tracking de uso y costos
METRICS_ENABLED=true

# Logging
# Opciones: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
